{
  "name": "@react-native-node-api-module-autolinking/example-node-api-module",
  "version": "1.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "A simple Node-API module in C that adds two numbers.",
  "gypfile": true,
  "files": [
    "index.js",
    "index.d.ts",
    "build/ios/Release",
    "build/macos/Release",
    "LICENSE-runtime-node-api.txt"
  ],
  "scripts": {
    "cmake:macos": "cmake-js compile --CDTARGET_PLATFORM=macos --out=build/macos",
    "cmake:ios": "cmake-js compile --CDTARGET_PLATFORM=ios --out=build/ios",
    "cmake:ios-sim": "cmake-js compile --CDTARGET_PLATFORM=ios-sim --out=build/ios-sim",
    "format:binding": "node scripts/format-binding.mjs",
    "format": "prettier . --write && npm run format:binding",
    "lint": "eslint .",
    "build": "node-gyp rebuild"
  },
  "devDependencies": {
    "cmake-js": "^7.3.0",
    "eslint": "^8.19.0",
    "node-api-headers": "^1.5.0",
    "node-gyp": "^11.1.0",
    "prettier": "2.8.8"
  }
}
